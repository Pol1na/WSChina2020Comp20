<Window x:Class="WSChina2020AppComp20.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WSChina2020AppComp20"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Frame x:Name="MainFrame" Grid.Row="1" 
               Navigated="MainFrame_Navigated" 
               NavigationUIVisibility="Hidden"
               ContentRendered="MainFrame_ContentRendered"></Frame>
        <Button Name="BackBtn" Style="{StaticResource BackButton}" VerticalAlignment="Top" Margin="100 10" Click="BackBtn_Click"></Button>
    </Grid>
</Window>
