<Page x:Class="WSChina2020AppComp20.Pages.CompetitionSkillsAdminPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WSChina2020AppComp20.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="CompetitionSkillsAdminPage">

    <Grid>
        <StackPanel Style="{StaticResource MSP}">
            <TextBlock Style="{StaticResource MainTB}">Competition Skills</TextBlock>
            <StackPanel Orientation="Horizontal">
                <TextBlock Style="{StaticResource SecondaryTB}">Event: </TextBlock>
                <ComboBox Name="EventsCB"></ComboBox>
                <Button Name="SearchBtn" Style="{StaticResource BackButton}" Click="SearchBtn_Click">Search</Button>
            </StackPanel>
            <TextBlock Style="{StaticResource SecondaryTB}" FontSize="10" HorizontalAlignment="Left">Skills in the selected event</TextBlock>
            <Button Name="AddSkillsBtn" Style="{StaticResource BackButton}" HorizontalAlignment="Right" Click="AddSkillsBtn_Click">Add Skills</Button>
        </StackPanel>
    </Grid>
</Page>
