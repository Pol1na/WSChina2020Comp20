<Page x:Class="WSChina2020AppComp20.Pages.AdminMenuPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WSChina2020AppComp20.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AdminMenuPage">

    <Grid>
        <TextBlock Style="{StaticResource MainTB}">Administrator Menu</TextBlock>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5">
            <TextBlock Name="HelloTB" FontSize="25" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="40"></TextBlock>
            <Button Name="EventBtn" Style="{StaticResource MainButton}" Margin="5" Click="EventBtn_Click">Event Managment</Button>
            <Button Name="CompetitorMngBtn" Style="{StaticResource MainButton}" Margin="5" Click="CompetitorMngBtn_Click">Competitor Managment</Button>
        </StackPanel>

    </Grid>
</Page>
