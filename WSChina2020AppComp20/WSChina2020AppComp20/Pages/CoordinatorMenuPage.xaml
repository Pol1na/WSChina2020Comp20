<Page x:Class="WSChina2020AppComp20.Pages.CoordinatorMenuPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WSChina2020AppComp20.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="CoordinatorMenuPage" MinHeight="450">

    <Grid>
        <TextBlock FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="20" >Coordinator Menu</TextBlock>
        <StackPanel Margin="20">
            <TextBlock Name="HelloTB" FontSize="25"  Margin="40"></TextBlock>
            <Button Style="{StaticResource MainButton}" Name="VolunteerBtn" Click="VolunteerBtn_Click" Margin="5">Volunteer Managment</Button>
            <Button Name="SponsorBtn" Style="{StaticResource MainButton}" Margin="5" Click="SponsorBtn_Click">Sponsorship Managment</Button>
            <Button Name="CompServiceBtn" Style="{StaticResource MainButton}" Margin="5" Click="CompServiceBtn_Click">Competition Service</Button>
        </StackPanel>

    </Grid>
</Page>
