<Page x:Class="WSChina2020AppComp20.Pages.ImportPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WSChina2020AppComp20.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ImportPage">

    <Grid>
        <StackPanel Style="{StaticResource MSP}">
            <TextBlock Style="{StaticResource MainTB}">Import Volunteers</TextBlock>
            <StackPanel Orientation="Horizontal">
                <TextBlock Style="{StaticResource SecondaryTB}">Excell list of volunteers: </TextBlock>
                <TextBox Name="ExcelTB" Width="200"></TextBox>
                <Button Name="BrowseBtn" Style="{StaticResource BackButton}" Click="BrowseBtn_Click">Browse</Button>
            </StackPanel>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="20">
                <Button Name="ImportBtn" Style="{StaticResource BackButton}" Click="ImportBtn_Click">Import</Button>
                <Button Name="CancelBtn" Style="{StaticResource BackButton}" Click="CancelBtn_Click">Cancel</Button>
            </StackPanel>
            <TextBlock Name="ImportTextBlock" Style="{StaticResource SecondaryTB}"></TextBlock>
        </StackPanel>
    </Grid>
</Page>
